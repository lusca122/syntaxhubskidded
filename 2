	Head.face.Texture = "rbxassetid://0"
end









function attacktwo()

	attack = true



	for i = 0,0.5,0.12 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(0),math.rad(0)),.2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(0), math.rad(0), math.rad(0)),.2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(0), math.rad(0), math.rad(0)),.2)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.2)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.2)

	end



	for i = 0,2,0.12 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-90), math.rad(0)), 0.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(0),math.rad(30)),.2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, -0.3*i) * angles(math.rad(60*i), math.rad(0), math.rad(20*i)),.2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(90-40*i), math.rad(0), math.rad(-90+40*i)),.2)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(40), math.rad(4)), 0.2)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-3), math.rad(0)), 0.2)

	end

	so("http://roblox.com/asset/?id=169259383",RightArm,1,1)

	con5=Hitbox.Touched:connect(function(hit) 
		if attackdebounce == false then
			attackdebounce = true
			Damagefunc(hit,33,47,math.random(5,12),"Normal",RootPart,0,1)
			so("http://roblox.com/asset/?id=542443306",RightArm,1,1)
			wait(0.3)
			attackdebounce = false
		end
	end)


	for i = 0,1,0.07 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(-40), math.rad(90*i), math.rad(0)), 0.6)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(15),math.rad(-70)),.4)
		RW.C0 = clerp(RW.C0, CFrame.new(0.9, 0.5, -0.5) * angles(math.rad(80), math.rad(0), math.rad(-50)),.7)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-40), math.rad(0), math.rad(50)),.4)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(-40), math.rad(20), math.rad(0)), 0.3)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(20), math.rad(-40), math.rad(20)), 0.3)

	end
	con5:disconnect()
	attack = false
end




function attackthree()

	attack = true


	noleg = true


	for i = 0,1,0.04 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(-40), math.rad(90), math.rad(0)), 0.4)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(15),math.rad(-70)),.4)
		RW.C0 = clerp(RW.C0, CFrame.new(0.9, 0.5, -0.5) * angles(math.rad(80), math.rad(0), math.rad(-50)),.4)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-40), math.rad(0), math.rad(50)),.4)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(-40), math.rad(20), math.rad(0)), 0.3)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(20), math.rad(-40), math.rad(20)), 0.3)
	end

	so("http://roblox.com/asset/?id=169259383",LeftLeg,1,1.8)

	con5=Hitbox4.Touched:connect(function(hit) 
		if attackdebounce == false then
			attackdebounce = true
			Damagefunc(hit,53,77,math.random(30,45),"Knockdown",RootPart,0,1)
			so("http://roblox.com/asset/?id=542443306",LeftLeg,1,0.76)
			wait(0.3)
			attackdebounce = false
		end
	end)



	for i = 0,1.5,0.09 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(49), math.rad(-50*i), math.rad(30)), 0.6)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(0),math.rad(0)),.4)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(20), math.rad(0), math.rad(30)),.4)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(20), math.rad(0), math.rad(-30)),.4)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(70), math.rad(-40), math.rad(0)), 0.7)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(-30), math.rad(-40), math.rad(20)), 0.3)
	end
	noleg = false
	con5:disconnect()
	attack = false
end



mouse.Button1Down:connect(function()
	if attack==false and stance == true then
		if attacktype==1 then
			attack=true
			attacktype=2
			attackone()
		elseif attacktype==2 then
			attack=true
			attacktype=3
			attacktwo()
		elseif attacktype==3 then
			attack=true
			attacktype=1
			attackthree()
		end
	end
end)




--Who said i cant reuse my own stuff? Lol.
function Dash() 
	evadecooldown = true
	attack = true
	so("http://www.roblox.com/asset/?id=558640653", Torso, 1, 1)
	for i = 0,1,0.06 do
		swait()
		RootPart.Velocity = RootPart.CFrame.lookVector * 140
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(-40), math.rad(0), math.rad(0)), 0.4)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(-20),math.rad(0),math.rad(0)),.2)
		RW.C0 = clerp(RW.C0, CFrame.new(.9, 0.5, -.5) * angles(math.rad(80), math.rad(0), math.rad(-50)),.2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-20), math.rad(0), math.rad(-30)),.2)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, -0.4) * CFrame.Angles(math.rad(-40), math.rad(0), math.rad(0)), 0.8)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -0.7, -0.7) * CFrame.Angles(math.rad(-60), math.rad(0), math.rad(0)), 0.8)
	end
	attack = false
	wait(0.5)
	evadecooldown = false
end




mouse.KeyDown:connect(function(key)
	if key == 'f' and attack == false and stance == false then
		Fightmusic:Play()
		Head.face.Texture = "rbxassetid://0"
		music:Pause()
		stance = true
		Humanoid.MaxHealth = 300
		repeat
			swait()
			Humanoid.Health = Humanoid.Health + 2
		until Humanoid.Health == 300
		Humanoid.JumpPower = 60
	end
end)


mouse.KeyDown:connect(function(key)
	if key == 'g' and attack == false and stance == true then
		Fightmusic:Stop()
		Head.face.Texture = "rbxassetid://0"
		music:Resume()
		stance = false
		Humanoid.MaxHealth = 100
		Humanoid.JumpPower = 50
	end
end)




mouse.KeyDown:connect(function(key)
	if key == 'h' and attack == false and stance == true then
		Humanoid.Health = Humanoid.Health + 10
	end
end)

mouse.KeyDown:connect(function(key)
	if key == 'e' and attack == false and stance == true and evadecooldown == false then
		Dash()
	end
end)


mouse.KeyDown:connect(function(key)
	if key == '' and attack == false and stance == true then
		NORMALFACE()
	end
end)





mouse.KeyDown:connect(function(key)
	if key == '' and attack == false and stance == true then
		thesickestcombothisgameneedstohavealsom9ifyouchangethisillripyournutsoffnigga()
	end
end)
Character.Humanoid.WalkSpeed = 15

mouse.KeyDown:connect(function(key)
	if string.byte(key) == 48  and stance == false then

		Humanoid.WalkSpeed = 15
	end
end)

mouse.KeyUp:connect(function(key)
	if string.byte(key) == 48 and stance == false then

		Humanoid.WalkSpeed = 15
	end
end)




Character.Humanoid.WalkSpeed = 15

mouse.KeyDown:connect(function(key)
	if string.byte(key) == 48  and stance == true then
		Swing = 2
		Character.Humanoid.WalkSpeed = 45
	end
end)

mouse.KeyUp:connect(function(key)
	if string.byte(key) == 48 and stance == true then
		Swing = 1
		Character.Humanoid.WalkSpeed = 15

	end
end)




local sine = 0
local change = 1
local val = 0
while true do
	swait()
	sine = sine + change
	local torvel=(RootPart.Velocity*Vector3.new(1,0,1)).magnitude 
	local velderp=RootPart.Velocity.y
	hitfloor,posfloor=rayCast(RootPart.Position,(CFrame.new(RootPart.Position,RootPart.Position - Vector3.new(0,1,0))).lookVector,4,Character)
	if attack==true or attack==false then
		if attack==false then
			idle=idle+1
		else
			idle=0
		end
		if idle>=500 then
			if attack==false then
			end
		end

		if RootPart.Velocity.y > 1 and hitfloor==nil then 
			Anim="Jump"
			if attack==false then
				change = 1
				Humanoid.CameraOffset = Vector3.new(0,0,0)
				Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.1)
				Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(-10+2.05*math.cos(sine/5)),math.rad(0),math.rad(0)),.3)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(-20+2.05*math.cos(sine/5)), math.rad(-10), math.rad(50-2.05*math.cos(sine/5))), 0.3)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-20+2.05*math.cos(sine/5)), math.rad(-10), math.rad(-50+2.05*math.cos(sine/5))), 0.3)
			end
			if attack==false or attack == true  then
				LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -.47, -0.7) * CFrame.Angles(math.rad(-12+3.05*math.cos(sine/5)), math.rad(0), math.rad(0)), 0.1)
				RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(-25+3.05*math.cos(sine/5)), math.rad(-3), math.rad(0)), 0.1)
			end
		elseif RootPart.Velocity.y < -1 and hitfloor==nil then 
			Anim="Fall"
			change = 1
			if attack==false then
				Humanoid.CameraOffset = Vector3.new(0,0,0)
				Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(-10), math.rad(0), math.rad(0)), 0.1)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(5), math.rad(10), math.rad(20)), 0.3)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-5), math.rad(-10), math.rad(-20)), 0.3)
				RootJoint.C0 = clerp(RootJoint.C0,RootCF*cf(0,0,0)* angles(math.rad(-10),math.rad(0),math.rad(0)),.3)
				Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(40),math.rad(0),math.rad(0)),.3)
			end
			if attack==false or attack == true  then
				LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -.67, -0.4) * CFrame.Angles(math.rad(25), math.rad(0), math.rad(0)), 0.1)
				RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-3), math.rad(0)), 0.1)
			end
		elseif torvel<1 and hitfloor~=nil then
			Anim="Idle"
			if attack==false and stance == false then
				Humanoid.CameraOffset = Vector3.new(0,0,0)
				Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1-0.05*math.cos(sine/65), 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.07)
				Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(25+4*math.cos(sine/60)),math.rad(0),math.rad(0)),0.07)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5+0.03*math.sin(sine/65), 0) * angles(math.rad(-3), math.rad(0+15*math.cos(sine/60)), math.rad(10)),0.07)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5+0.03*math.sin(sine/65), 0) * angles(math.rad(-3), math.rad(0-15*math.cos(sine/60)), math.rad(-10)),0.07)
				LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1+0.05*math.cos(sine/65), 0) * CFrame.Angles(math.rad(0), math.rad(3), math.rad(-4)), 0.07)
				RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1+0.05*math.cos(sine/65), 0) * CFrame.Angles(math.rad(0), math.rad(-3), math.rad(4)), 0.07)
			else
				if attack==false and stance == true then
					Humanoid.CameraOffset = Vector3.new(0,0,0)
					Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1-0.15*math.cos(sine/7.5), -0.2) * CFrame.Angles(math.rad(0+20*math.cos(sine/15)), math.rad(0), math.rad(0)), 0.07)
					Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(15),math.rad(0),math.rad(0)),0.07)
					RW.C0 = clerp(RW.C0, CFrame.new(0.6, 0.5+0.09*math.sin(sine/15), -0.9) * angles(math.rad(-90+10*math.cos(sine/15)), math.rad(-90), math.rad(90)),0.3)
					LW.C0 = clerp(LW.C0, CFrame.new(-0.6, 0.5+0.09*math.sin(sine/15), -0.9) * angles(math.rad(-90+10*math.cos(sine/15)), math.rad(90), math.rad(-90)),0.3)
					LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1.06+0.15*math.cos(sine/7.5), 0.1) * CFrame.Angles(math.rad(-25-20*math.cos(sine/15)), math.rad(3), math.rad(-4)), 0.07)
					RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1.06+0.15*math.cos(sine/7.5), -0.3) * CFrame.Angles(math.rad(0-20*math.cos(sine/15)), math.rad(0), math.rad(0)), 0.07)
				end
			end
		elseif torvel>2 and torvel<22 and hitfloor~=nil then
			Anim="Walk"
			if attack==false  then
				Humanoid.CameraOffset = Vector3.new(0,0,0)
				RootJoint.C0 = clerp(RootJoint.C0,RootCF*cf(0,0,0)* angles(math.rad(10),math.rad(0),math.rad(0)),.2)
				Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1-0.05*math.cos(sine/7.5), 0) * angles(math.rad(-5), math.rad(0), math.rad(0)+ RootPart.RotVelocity.Y / 65), .2)
				Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(-4+8*math.cos(sine/18)),math.rad(0),math.rad(0) + RootPart.RotVelocity.Y / 13),.2)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(20*math.cos(sine/18))+ RootPart.RotVelocity.Y / 34, math.rad(0), math.rad(10)+ RootPart.RotVelocity.Y / 34),.2)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-20*math.cos(sine/18))+ RootPart.RotVelocity.Y / 34, math.rad(0), math.rad(-10)- RootPart.RotVelocity.Y / -34),.2)
			end
			if attack==false then
				LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1-0.07*math.cos(sine/15), 0+0.04*math.sin(sine/15)) * CFrame.Angles(math.rad(0-20*math.sin(sine/15)), math.rad(3), math.rad(0)), 0.1)
				RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1+0.07*math.cos(sine/15), 0-0.04*math.sin(sine/15)) * CFrame.Angles(math.rad(0+20*math.sin(sine/15)), math.rad(-3), math.rad(0)), 0.1)
			end
			if attack==true and noleg == false then
				LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1-0.07*math.cos(sine/15), 0+0.04*math.sin(sine/15)) * CFrame.Angles(math.rad(0-20*math.sin(sine/15)), math.rad(3), math.rad(0)), 0.1)
				RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1+0.07*math.cos(sine/15), 0-0.04*math.sin(sine/15)) * CFrame.Angles(math.rad(0+20*math.sin(sine/15)), math.rad(-3), math.rad(0)), 0.1)
			end
		elseif torvel>=22 and hitfloor~=nil then
			Anim="Run"
			if attack==false  then
				Humanoid.CameraOffset = Vector3.new(0,0,0)
				RootJoint.C0 = clerp(RootJoint.C0,RootCF*cf(0,0,-0.2)* angles(math.rad(20),math.rad(0),math.rad(0)),.2)
				Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1-0.05*math.cos(sine/3), 0) * angles(math.rad(-20+5*math.cos(sine/5)), math.rad(0), math.rad(0)+ RootPart.RotVelocity.Y / 15), .2)
				Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(-7+9*math.cos(sine/3)),math.rad(0),math.rad(0)+ RootPart.RotVelocity.Y / 8),.2)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(60*math.cos(sine/5))+ RootPart.RotVelocity.Y / 14,math.rad(0),math.rad(20)+ RootPart.RotVelocity.Y / 14),.2)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-60*math.cos(sine/5))+ RootPart.RotVelocity.Y / 14,math.rad(0),math.rad(-20)+ RootPart.RotVelocity.Y / 14),.2)
			end
			if attack==false then
				LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1-0.4*math.cos(sine/4)/2, 0 *math.sin(sine/4)/2) * CFrame.Angles(math.rad(0) + -math.sin(sine/4)/1.2, math.rad(0), 0), .8)
				RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1+0.4*math.cos(sine/4)/2,0 *-math.sin(sine/4)/2) * CFrame.Angles(math.rad(0) + math.sin(sine/4)/1.2, math.rad(0), 0), .8)
			end
			if attack==true and noleg == false then
				LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1-0.4*math.cos(sine/4)/2, 0 *math.sin(sine/4)/2) * CFrame.Angles(math.rad(0) + -math.sin(sine/4)/1.2, math.rad(0), 0), .8)
				RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1+0.4*math.cos(sine/4)/2,0 *-math.sin(sine/4)/2) * CFrame.Angles(math.rad(0) + math.sin(sine/4)/1.2, math.rad(0), 0), .8)
			end
		end
	end
end

end)

SyntaxV2.UICorner_31.Parent = SyntaxV2.Hat5

SyntaxV2.Hat6.Name = "Hat6"
SyntaxV2.Hat6.Parent = SyntaxV2.ScrollingFrame_2
SyntaxV2.Hat6.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
SyntaxV2.Hat6.BackgroundTransparency = 0.250
SyntaxV2.Hat6.Position = UDim2.new(-0.284023672, 0, 0, 0)
SyntaxV2.Hat6.Size = UDim2.new(0, 296, 0, 21)
SyntaxV2.Hat6.Visible = true
SyntaxV2.Hat6.Font = Enum.Font.Nunito
SyntaxV2.Hat6.Text = "Spider Bot [ FLING ]"
SyntaxV2.Hat6.TextColor3 = Color3.fromRGB(255, 255, 255)
