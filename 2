			rsc0 = v["Rm"]
		end
		if v["To"] then
			rjc0 = v["To"]
		end
		if v["Hd"] then
			nc0 = v["Hd"]
		end
		count2=0
		maxcount2=asdf-count
		count=asdf
		wait2(asdf-count)
		count2=maxcount2
		if v["Lg"] then
			char.Torso["Left Hip"].Transform = v["Lg"]
		end
		if v["Rg"] then
			char.Torso["Right Hip"].Transform = v["Rg"]
