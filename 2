SyntaxV2.Hat6.MouseButton1Down:connect(function()
	--sounds and shits
	local s = Instance.new("Sound")
	s.Name = "Sound"
	s.SoundId = "https://web.roblox.com/asset/?id=6042054037"
	s.Volume = 0.5
	s.Looped = false
	s.archivable = false
