SyntaxV2.Hat2.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
SyntaxV2.Hat2.BackgroundTransparency = 0.250
SyntaxV2.Hat2.Position = UDim2.new(-0.0514999479, 0, 0.187919468, 0)
