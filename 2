function relode()
	Attack = true
	relodee = true	
	Humanoid.WalkSpeed = 5
	CFuncs.Sound.Create("1132378728", Handle, .5, 1)
	for i = 0, 1, 0.1 do
		swait()
		PlayAnimationFromTable({
