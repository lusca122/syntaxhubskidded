		end

	end
	ok2:Play()
	attack = false
	grab:BreakJoints()
	wait(8)
	Fightmusic:Play()
end

function NORMALFACE()
	attack = true

	for i = 0,2,0.06 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(20), math.rad(90), math.rad(0)), 0.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(0),math.rad(-30)),.2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(20), math.rad(0), math.rad(60)),.4)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(20), math.rad(0), math.rad(-30)),.4)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(3), math.rad(0)), 0.2)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -0.5, -0.6) * CFrame.Angles(math.rad(20), math.rad(0), math.rad(0)), 0.2)

	end

	--local GGyro = Instance.new("BodyPosition")
	local grab = nil
	for i, v in pairs(FindNearestTorso(Torso.CFrame.p, 5)) do
		if v:FindFirstChild('Torso') then
			Grabbed = true
			grab = v
		end
	end


	if Grabbed == true and grab.Name == "KillerDarkness0105" then
		Grabbed = false
		attack = false
		for i = 1,300 do
			wait(0.4)
			so("http://www.roblox.com/asset/?id=170040190", Head, 1, 1)
			Character:BreakJoints()
		end
		wait(2)
	end



	if grab == nil then
		print("Get to a vaild torso.")
		attack = false
	end

	if Grabbed == true then
		grab.Humanoid.PlatformStand = true
		wait(1)
		grab.Head.CFrame = Hitbox2.CFrame * CFrame.Angles(math.rad(0),math.rad(0),math.rad(0))


		for i = 0,3,0.12 do
			swait()
			Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-90), math.rad(0)), 0.1)
			Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(0),math.rad(60)),.1)
			RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(20), math.rad(0), math.rad(30)),.2)
			LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, -.3) * angles(math.rad(20), math.rad(-40*i), math.rad(-90)),.2)
			LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(40), math.rad(-4)), 0.3)
			RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-3), math.rad(0)), 0.3)
		end
	end
	grab.Head.CFrame = Hitbox2.CFrame * CFrame.Angles(math.rad(0),math.rad(0),math.rad(0))
	grab.Torso.Anchored = true


	for i = 0,3,0.06 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-90), math.rad(0)), 0.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(0),math.rad(60)),.1)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(20), math.rad(0), math.rad(30)),.2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, -.3) * angles(math.rad(90), math.rad(0), math.rad(-90)),.2)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(40), math.rad(-4)), 0.3)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-3), math.rad(0)), 0.3)
	end



	for i = 1, 3 do
		so("http://www.roblox.com/asset/?id=138903071", handee2, 1, 1)
		local part = Instance.new("Part",handee2)
		part.Shape = "Ball"
		part.Anchored = true
		part.CanCollide = false
		part.BrickColor = BrickColor.new"Really red"
		part.Material = Enum.Material.Neon
		part.Size = Vector3.new(1,1,1)
		part.CFrame = handee2.CFrame
		part.TopSurface,part.BottomSurface = 10,10
		for i = 0,1,.3 do
			part.Size = part.Size+ Vector3.new(0.25,0.25,0.25)
			part.Transparency = i
			wait()
		end
		part:destroy()
	end


	for i = 0,3,0.06 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-90), math.rad(0)), 0.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(0),math.rad(60)),.1)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(20), math.rad(0), math.rad(30)),.2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, -.3) * angles(math.rad(90), math.rad(0), math.rad(-90)),.2)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(40), math.rad(-4)), 0.3)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-3), math.rad(0)), 0.3)
	end

	Fightmusic:Pause()
	Head.face.Texture = "rbxassetid://00"
	so("http://www.roblox.com/asset/?id=992192914", Head, 3, 1)
	wait(2)
	grab.Head.face.Texture = "rbxassetid://00"
	wait(1.7)

	for i = 1, 40 do
		so("http://www.roblox.com/asset/?id=390473202", Character, 1, 1)
		local part = Instance.new("Part",grab.Torso)
		part.Shape = "Ball"
		part.Anchored = true
		part.CanCollide = false
		part.BrickColor = BrickColor.new"Really red"
		part.Material = Enum.Material.Neon
		part.Size = Vector3.new(1,1,1)
		part.CFrame = grab.Torso.CFrame
		part.TopSurface,part.BottomSurface = 10,10
		for i = 0,1,.83 do
			part.Size = part.Size+ Vector3.new(5.25,5.25,5.25)
			part.Transparency = i
			wait()
		end
		part:destroy()
	end

	local Blood = Instance.new("ParticleEmitter", grab.Torso)
	Blood.VelocitySpread = 180
	Blood.Lifetime = NumberRange.new(1)
	Blood.Texture = "http://roblox.com/asset/?id=771221224"
	Blood.Speed = NumberRange.new(3.5)
	Blood.Size = NumberSequence.new({NumberSequenceKeypoint.new(0, 1), NumberSequenceKeypoint.new(1, 1)})
	Blood.RotSpeed = NumberRange.new(-45, 45)
	Blood.Rate = 1280
	Blood.Rotation = NumberRange.new(-45, 45)
	Blood.Acceleration = Vector3.new(1,-3,1)
	Blood.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0, 0), NumberSequenceKeypoint.new(0.701, 0), NumberSequenceKeypoint.new(1, 1)})
	Blood.LightEmission = 0.8
	Blood.Color = ColorSequence.new(Color3.new(0.2, 0, 0), Color3.new(0.3,0,0))


	local Blood2 = Instance.new("ParticleEmitter", grab.Torso)
	Blood2.VelocitySpread = 280
	Blood2.Lifetime = NumberRange.new(1)
	Blood2.Texture = "http://roblox.com/asset/?id=771221224"
	Blood2.Speed = NumberRange.new(3.5)
	Blood2.Size = NumberSequence.new({NumberSequenceKeypoint.new(0, 1), NumberSequenceKeypoint.new(1, 1)})
	Blood2.RotSpeed = NumberRange.new(-45, 45)
	Blood2.Rate = 1280
	Blood2.Rotation = NumberRange.new(-45, 45)
	Blood2.Acceleration = Vector3.new(1,-3,1)
	Blood2.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0, 0), NumberSequenceKeypoint.new(0.701, 0), NumberSequenceKeypoint.new(1, 1)})
	Blood2.LightEmission = 0.8
	Blood2.Color = ColorSequence.new(Color3.new(0.1, 0, 0), Color3.new(0.4,0,0))

	grab.Torso.Anchored = false

	Blood.Enabled = true
	Blood2.Enabled = true
	grab:BreakJoints()
	grab.Head.face.Texture = "rbxassetid://00"
	so("http://www.roblox.com/asset/?id=00", grab.Head, 1, 1)
	wait(3)

	so("http://www.roblox.com/asset/?id=00", Head, 10, 1)
	wait(0.2)

	so("http://www.roblox.com/asset/?id=00", Head, 10, 1)
	wait(0.2)

	so("http://www.roblox.com/asset/?id=00", Head, 10, 1)
	wait(0.2)

	so("http://www.roblox.com/asset/?id=00", Head, 10, 1)
	wait(0.2)
	so("http://www.roblox.com/asset/?id=000", Head, 10, 1)
	wait(0.2)

	so("http://www.roblox.com/asset/?id=00", Head, 10, 1)
	wait(0.2)

	so("http://www.roblox.com/asset/?id=00", Head, 10, 1)
	wait(0.2)

	so("http://www.roblox.com/asset/?id=00", Head, 10, 1)
	wait(2)

	ok:Play()
	wait(2)

	for i = 0,53,0.06 do
		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-90), math.rad(0)), 0.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(-40),math.rad(0),math.rad(60)),.1)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(-90), math.rad(-90), math.rad(90)),0.3)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-90), math.rad(90), math.rad(-90)),0.3)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.3)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.3)




		swait()
		Torso.Weld.C0 = clerp(Torso.Weld.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(0), math.rad(-90), math.rad(0)), 0.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(-60),math.rad(0),math.rad(-60)),.1)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(90), math.rad(90), math.rad(-90)),0.3)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(90), math.rad(-90), math.rad(90)),0.3)
		LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.3)
		RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.3)

	end

	wait()
