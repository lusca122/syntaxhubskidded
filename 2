function hedbap()
	Attack = true
	for i = 0, 1, 0.1 do
		swait()
		PlayAnimationFromTable({
			CFrame.new(-0, -0.115550078, 0.250714004, 1, 0, -0, 0, 0.939692736, -0.342019886, 0, 0.342019916, 0.939692736) * CFrame.new(0, 0, 0) * CFrame.Angles(0, 0, 0), 
