	if SyntaxRig.Humanoid.FloorMaterial~=Enum.Material.Air then
		SyntaxRig.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
		game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').Sit = false
	end
end)
