Syntax99.Hat5.MouseButton1Down:connect(function()
	local s = Instance.new("Sound")
	s.Name = "Sound"
	s.SoundId = "https://web.roblox.com/asset/?id=1673280232"
	s.Volume = 0.5
	s.Looped = false
	s.archivable = false
	s.Parent = game.Workspace
	s:play()
	Syntax99.Hat5.Size = UDim2.new(0, 236, 0, 21)
	wait(0.1)
	Syntax99.Hat5.Size = UDim2.new(0, 296, 0, 21)
	local hat = game:GetService("Workspace")[game.Players.LocalPlayer.Name]["VoidLordScythe"]


