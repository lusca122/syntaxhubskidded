	if rig.Humanoid.FloorMaterial~=Enum.Material.Air then
		rig.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
		game.Players.LocalPlayer.Character.Dummy:FindFirstChildOfClass('Humanoid').Sit = false
	end
end)
