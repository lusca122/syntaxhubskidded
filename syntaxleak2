for i,v in pairs(hed:children()) do
	if v:IsA("Sound") then
		v:Destroy()
	end
end
----------------------------------------------------
function HasntTouched(plrname)
	local ret = true
	for _, v in pairs(Touche) do
		if v == plrname then
			ret = false
		end
	end
	return ret
end
