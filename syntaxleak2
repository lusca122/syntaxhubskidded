local char = workspace.Dummy

Instance.new("Attachment",char["Right Arm"])
Instance.new("Attachment",char["Red Sci-Fi Scattergun"].Handle)
Instance.new("AlignPosition",char["Red Sci-Fi Scattergun"].Handle)
Instance.new("AlignOrientation", char["Red Sci-Fi Scattergun"].Handle)
char["Red Sci-Fi Scattergun"].Handle.AccessoryWeld.Parent = nil
char["Red Sci-Fi Scattergun"].Handle.AlignPosition.Attachment0 = char["Red Sci-Fi Scattergun"].Handle.Attachment
char["Red Sci-Fi Scattergun"].Handle.AlignOrientation.Attachment0 = char["Red Sci-Fi Scattergun"].Handle.Attachment
char["Red Sci-Fi Scattergun"].Handle.AlignPosition.Attachment1 = char["Right Arm"].Attachment
char["Red Sci-Fi Scattergun"].Handle.AlignOrientation.Attachment1 = char["Right Arm"].Attachment
char["Red Sci-Fi Scattergun"].Handle.AlignPosition.Responsiveness = 200
char["Red Sci-Fi Scattergun"].Handle.AlignOrientation.Responsiveness = 200
char["Red Sci-Fi Scattergun"].Handle.Attachment.Orientation = Vector3.new(5.38, 35.7, -30.6)
char["Red Sci-Fi Scattergun"].Handle.Attachment.Position = Vector3.new(0.7, 1.184, -0.065)
char["Red Sci-Fi Scattergun"].Handle.AlignPosition.RigidityEnabled = true
char["Red Sci-Fi Scattergun"].Handle.AlignOrientation.RigidityEnabled = true



local Particle = IT("ParticleEmitter",nil)
Particle.Enabled = false
Particle.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,0.3),NumberSequenceKeypoint.new(0.3,0),NumberSequenceKeypoint.new(1,1)})
Particle.LightEmission = 0.5
Particle.Rate = 150
Particle.ZOffset = 0.2
Particle.Rotation = NumberRange.new(-180, 180)
Particle.RotSpeed = NumberRange.new(-180, 180)
Particle.Texture = "http://www.roblox.com/asset/?id=304437537"
Particle.Color = ColorSequence.new(C3(0,1,0),C3(0,1.1,0))

--ParticleEmitter({Speed = 5, Drag = 0, Size1 = 1, Size2 = 5, Lifetime1 = 1, Lifetime2 = 1.5, Parent = Torso, Emit = 100, Offset = 360, Enabled = false})
function ParticleEmitter(Table)
	local PRTCL = Particle:Clone()
	local Speed = Table.Speed or 5
	local Drag = Table.Drag or 0
	local Size1 = Table.Size1 or 1
	local Size2 = Table.Size2 or 5
	local Lifetime1 = Table.Lifetime1 or 1
	local Lifetime2 = Table.Lifetime2 or 1.5
	local Parent = Table.Parent or Torso
	local Emit = Table.Emit or 100
	local Offset = Table.Offset or 360
	local Acel = Table.Acel or VT(0,0,0)
	local Enabled = Table.Enabled or false
	PRTCL.Parent = Parent
	PRTCL.Size = NumberSequence.new(Size1,Size2)
	PRTCL.Lifetime = NumberRange.new(Lifetime1,Lifetime2)
	PRTCL.Speed = NumberRange.new(Speed)
	PRTCL.VelocitySpread = Offset
	PRTCL.Drag = Drag
	PRTCL.Acceleration = Acel
	if Enabled == false then
		PRTCL:Emit(Emit)
		Debris:AddItem(PRTCL,Lifetime2)
	else
		PRTCL.Enabled = true
	end
	return PRTCL
end

for _, c in pairs(CharacterA:GetChildren()) do
	if c.ClassName == "Part" then
		c.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
	end
end

