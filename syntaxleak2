					RIGHTLEGLERP.C0 = RIGHTLEGLERP.C0:lerp(CFrame.new(-.5,2,0) * CFrame.Angles(0,0,0),.25)
					swait()
				end
				ws = 14
				barrol:Remove()
				attacking = false
			end)()
		end
	end
end)

