function HAAAAA() --KONO POWA
	attack = true
	hum.WalkSpeed = 1.01
	Character.Head.face.Texture = "rbxassetid://305068389"
	chargeup.Pitch = 1
	for i = 0,7,0.1 do
		swait()
		rootj.C0 = clerp(rootj.C0, RootCF * CF(0, 0, -0.1 + 0.1) * angles(Rad(0), Rad(0), Rad(0)), 0.15)
