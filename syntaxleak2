	tra.Texture = "rbxassetid://347730682"
	tra.LightEmission = 0.8
	tra.Color = ColorSequence.new(Color3.new(1, 0, 0))
	tra.Rate = 250
	tra.Rotation = NumberRange.new(-5, 5)
