function nocol(same)
	for i,v in pairs(Character:GetDescendants()) do
		if v:IsA("BasePart") then
			HILOL=Instance.new("NoCollisionConstraint",v)
			HILOL.Part0 = v
			HILOL.Part1 = same
		end
	end
end
for i,v in pairs(rig:GetDescendants()) do
	if v:IsA("BasePart") then
		nocol(v)
	end
end


